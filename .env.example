APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

VITE_APP_NAME="${APP_NAME}"
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# WooCommerce Integration
WOOCOMMERCE_STORE_URL=https://yourstore.com
WOOCOMMERCE_CONSUMER_KEY=ck_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
WOOCOMMERCE_CONSUMER_SECRET=cs_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
WOOCOMMERCE_API_VERSION=wc/v3

# Sync Configuration
WOOCOMMERCE_SYNC_ENABLED=true
WOOCOMMERCE_SYNC_INTERVAL=15
WOOCOMMERCE_SYNC_DIRECTION=bidirectional
WOOCOMMERCE_SYNC_PRODUCTS=true
WOOCOMMERCE_SYNC_ORDERS=true
WOOCOMMERCE_SYNC_CUSTOMERS=true
WOOCOMMERCE_SYNC_INVENTORY=true
WOOCOMMERCE_SYNC_CATEGORIES=true
WOOCOMMERCE_BATCH_SIZE=100
WOOCOMMERCE_RETRY_FAILED=true
WOOCOMMERCE_MAX_RETRY_ATTEMPTS=3

# Webhooks
WOOCOMMERCE_WEBHOOKS_ENABLED=true
WOOCOMMERCE_WEBHOOK_SECRET=your_webhook_secret_here

# Product Settings
WOOCOMMERCE_SYNC_IMAGES=true
WOOCOMMERCE_SYNC_VARIATIONS=true
WOOCOMMERCE_DEFAULT_PRODUCT_STATUS=publish

# Inventory Settings
WOOCOMMERCE_REALTIME_INVENTORY=true
WOOCOMMERCE_SYNC_ON_SALE=true
WOOCOMMERCE_LOW_STOCK_THRESHOLD=10
WOOCOMMERCE_OUT_OF_STOCK_THRESHOLD=0

# Error Handling
WOOCOMMERCE_LOG_REQUESTS=false
WOOCOMMERCE_LOG_ERRORS=true
WOOCOMMERCE_EMAIL_ON_FAILURE=false
WOOCOMMERCE_NOTIFICATION_EMAIL=admin@example.com
WOOCOMMERCE_SLACK_NOTIFICATIONS=false
WOOCOMMERCE_SLACK_WEBHOOK_URL=

# Performance
WOOCOMMERCE_CACHE_ENABLED=true
WOOCOMMERCE_CACHE_TTL=3600
WOOCOMMERCE_USE_QUEUE=true
WOOCOMMERCE_QUEUE_CONNECTION=database
WOOCOMMERCE_TIMEOUT=30
WOOCOMMERCE_MAX_CONCURRENT_REQUESTS=5
